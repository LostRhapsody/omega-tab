{
  "name": "omega_tab_client",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx vite",
    "build": "bunx vite build",
    "preview": "bunx vite preview",
    "type-check": "bunx vue-tsc --noEmit",
    "lint": "bunx oxlint src",
    "format": "bunx oxfmt src",
    "check": "bun run lint && bun run type-check",
    "verify": "bun run type-check && bun run lint && bun run format && bun run build",
    "verify:fast": "bun run type-check && bun run lint && bun run format",
    "test:unit": "vitest",
    "test:run": "vitest run",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.1",
    "@types/lodash": "^4.17.15",
    "@unhead/vue": "^1.11.20",
    "fuse.js": "^7.0.0",
    "lodash": "^4.17.21",
    "pinia": "^2.3.1",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@tsconfig/node22": "^22.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^22.13.5",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "cypress": "^14.0.1",
    "jsdom": "^26.0.0",
    "oxfmt": "^0.26.0",
    "oxlint": "^1.41.0",
    "start-server-and-test": "^2.0.10",
    "tailwindcss": "4",
    "typescript": "~5.7.3",
    "vite": "^6.0.11",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vue-devtools": "^7.7.1",
    "vitest": "^3.0.4",
    "vue-tsc": "^2.2.0"
  }
}
